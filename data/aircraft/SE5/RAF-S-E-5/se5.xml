  <!-- Une ombre pour OSG -->
  <model>
    <path>Aircraft/RAF-S-E-5/Models/shadow.xml</path>
    <offsets>
      <x-m>  0 </x-m>
      <y-m>  0 </y-m>
      <z-m> -1.068 </z-m>
    </offsets>
  </model>

  <!-- Intérieur -->
  <model>
    <path>Aircraft/RAF-S-E-5/Models/Interior/interior.xml</path>
  </model>

  <!-- Cranking effects -->
  <!-- Left -->
  <model>
    <path>Aircraft/RAF-S-E-5/Models/Effects/cranking/cranking.xml</path>
    <offsets>
      <x-m> -0.158 </x-m>
      <y-m> -0.319 </y-m>
      <z-m>  0.322 </z-m>
      <roll-deg>      0 </roll-deg>
      <pitch-deg>     0 </pitch-deg>
      <heading-deg> -30 </heading-deg>
    </offsets>
  </model>

  <!-- Right -->
  <model>
    <path>Aircraft/RAF-S-E-5/Models/Effects/cranking/cranking.xml</path>
    <offsets>
      <x-m> -0.158 </x-m>
      <y-m>  0.319 </y-m>
      <z-m>  0.322 </z-m>
      <roll-deg>     0 </roll-deg>
      <pitch-deg>    0 </pitch-deg>
      <heading-deg> 30 </heading-deg>
    </offsets>
  </model>

  <!-- Effets sur le sol -->
  <!-- Roue gauche -->
  <model>
    <path>Aircraft/RAF-S-E-5/Models/Effects/ground/ground0.xml</path>
    <offsets>
      <x-m> -1.827 </x-m>
      <y-m> -0.679 </y-m>
      <z-m> -1.450 </z-m>
    </offsets>
  </model>

