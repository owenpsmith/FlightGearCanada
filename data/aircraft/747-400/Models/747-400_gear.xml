<!-- NOSE -->

<animation>
 <name>GearNose</name>
 <object-name>GearNoseTorqueLinkLow</object-name>
 <object-name>GearNoseTorqueLinkUp</object-name>
 <object-name>GearNoseTorqueLinkUpp</object-name>
 <object-name>GearNoseMainStrutCyli</object-name>
 <object-name>GearNoseMainStrutColl</object-name>
 <object-name>GearNoseMainStrutPist</object-name>
 <object-name>GearNoseWheels</object-name>
 <object-name>Mesh20</object-name>
 <object-name>Mesh22</object-name>
 <object-name>Mesh25</object-name>
 <object-name>Plane</object-name>
</animation>

<animation>
 <type>select</type>
 <object-name>GearNose</object-name>
 <condition>
  <greater-than>
   <property>gear/gear[0]/position-norm</property>
   <value>0.0</value>
  </greater-than>
 </condition>
</animation>

<animation>
 <type>rotate</type>
 <object-name>GearNose</object-name>
 <property>gear/gear[0]/position-norm</property>
 <interpolation>
  <entry>
   <ind>0.0</ind>
   <dep>115</dep>
  </entry>
  <entry>
   <ind>0.1</ind>
   <dep>115</dep>
  </entry>
  <entry>
   <ind>0.9</ind>
   <dep>0</dep>
  </entry>
  <entry>
   <ind>1.0</ind>
   <dep>0</dep>
  </entry>
 </interpolation>
 <center>
  <x-m>-21.991</x-m>
  <y-m>0.0</y-m>
  <z-m>-2</z-m>
 </center>
 <axis>
  <x>0</x>
  <y>1</y>
  <z>0</z>
 </axis>
</animation>

<animation>
 <type>rotate</type>
 <object-name>GearNoseTorqueLinkLow</object-name>
 <object-name>GearNoseTorqueLinkUpp</object-name>
 <object-name>GearNoseTorqueLinkUpp</object-name>
 <object-name>GearNoseMainStrutPist</object-name>
 <object-name>GearNoseWheels</object-name>
 <condition>
  <less-than>
   <property>position/altitude-agl-ft</property>        <!-- weight sensor -->
   <value>30.0</value>
  </less-than>
 </condition>
 <property>gear/gear[0]/steering-norm</property>
 <factor>-70</factor>
 <center>
  <x-m>-21.991</x-m>
  <y-m>0.0</y-m>
  <z-m>-2</z-m>
 </center>
 <axis>
  <x>0</x>
  <y>0</y>
  <z>1</z>
 </axis>
</animation>

<animation>
